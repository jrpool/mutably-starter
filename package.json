{
  "name": "mutably-starter",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "lint":
      "eslint ./server.js ./public/script.js ./views/text/*.js; exit 0",
    "start": "node server1.js",
    "star2": "node server2.js",
    "localstart1": "nodemon server1.js",
    "localstart2": "nodemon server2.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["API", "AJAX"],
  "author": "Danielle Gellis and Jonathan Pool",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.17.1",
    "ejs": "^2.5.6",
    "express": "^4.15.2"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "babel-preset-es2015": ">=6.24.1",
    "babel-register": ">=6.24.1",
    "eslint": "^4.0.0"
  },
  "engines": {
    "node": ">=8.1.2"
  },
  "bugs": {
    "url": "https://github.com/jrpool/mutably-starter.git/issues"
  },
  "homepage": "https://github.com/jrpool/mutably-starter.git/index.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/jrpool/mutably-starter.git"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true,
      "es6": true,
      "jquery": true,
      "mocha": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "arrow-spacing": [
        "error",
        {
          "before": true,
          "after": true
        }
      ],
      "indent": [
        "error",
        2,
        {
          "MemberExpression": 0,
          "SwitchCase": 1
        }
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "no-array-constructor": [
        "error"
      ],
      "no-cond-assign": [
        "off"
      ],
      "no-control-regex": [
        "off"
      ],
      "no-console": [
        "off"
      ],
      "no-const-assign": [
        "error"
      ],
      "no-new-object": [
        "error"
      ],
      "no-var": [
        "off"
      ],
      "no-whitespace-before-property": [
        "error"
      ],
      "prefer-const": [
        "warn"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "space-before-blocks": [
        "error",
        "always"
      ],
      "space-before-function-paren": [
        "error",
        "never"
      ],
      "space-in-parens": [
        "error",
        "never"
      ],
      "space-unary-ops": [
        "error",
        {
          "words": true,
          "nonwords": false
        }
      ],
      "spaced-comment": [
        "error",
        "always",
        {
          "markers": [
            "/",
            "*"
          ]
        }
      ]
    }
  }
}
